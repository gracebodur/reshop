{
  "name": "reshop",
  "version": "1.0.0",
  "description": "Full-stack ecommerce app made with create-react-app client and Node server",
  "engines": {
    "node": "14.16.0"
  },
  "scripts": {
    "start": "node server",
    "dev:server": "nodemon server",
    "dev:client": "cd client && npm start",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "cd client && npm install && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gracebodur/reshop.git"
  },
  "author": "Marie Grace Bodur",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gracebodur/reshop/issues"
  },
  "homepage": "https://github.com/gracebodur/reshop#readme",
  "devDependencies": {
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.7"
  },
  "dependencies": {
    "browserslist": "^4.16.7",
    "color-string": "^1.6.0",
    "dns-packet": "^1.3.4",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "glob-parent": "^5.1.2",
    "hosted-git-info": "^2.8.9",
    "is-svg": "^4.3.1",
    "path-parse": "^1.0.7",
    "postcss": "^8.3.6",
    "ssri": "^6.0.2",
    "stripe": "^8.151.0",
    "tar": "^6.1.8",
    "url-parse": "^1.5.3",
    "use-shopping-cart": "^2.4.3",
    "ws": "^7.5.3"
  }
}
